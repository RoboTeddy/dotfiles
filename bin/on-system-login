#!/usr/bin/env bash


# launchd executes this whenever the user logs in

# Call internet-off unless we're in the period when internet should be on
# Internet should be on from 5pm to 7pm

cd "$(dirname "$0")" # cd to directory this file is in

time=$(date +%k%M)
if [[ "$time" -le 1700 ]] || [[ "$time" -ge 1900 ]];then
  ./internet-off
fi
